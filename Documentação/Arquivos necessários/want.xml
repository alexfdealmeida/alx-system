	<target name="prepare">
		<mkdir dir="${bin}" />
		<mkdir dir="${zipdir}" />
		<property name="final" value="%{final}" />
	</target>
	<target name="clean">
		<delete dir="${basedir}" basedir="${basedir}">
			<include name="*.dcu" />
			<include name="**/*.dcu" />
		</delete>
	</target>
	<target name="clean_all">
		<delete dir="${bin}" basedir="${bin}">
			<include name="*.exe" />
			<include name="*.map" />
		</delete>
		<delete dir="${zipdir}" basedir="${zipdir}">
			<include name="*.exe" />
		</delete>
		<delete dir="${basedir}" basedir="${basedir}">
			<include name="*.dcu" />
			<include name="**/*.dcu" />
		</delete>
		<delete dir="@{${HEAD}/Componentes}" basedir="@{${HEAD}/Componentes">
			<include name="*.dcu" />
			<include name="**/*.dcu" />
		</delete>
	</target>
	<!-- Desconecta DPrincipal da base de dados -->
	<target name="closedb">
		<edit file="${basedir}/SAgrTran/Dprincipal.dfm">
			<search pattern="Connected = True">
				<delete />
			</search>
			<write all="True" file="${basedir}/SAgrTran/Dprincipal.dfm" />
		</edit>
		<edit file="${basedir}/SAgrAdap/Dprincipal.dfm">
			<search pattern="Connected = True">
				<delete />
			</search>
			<write file="${basedir}/SAgrAdap/Dprincipal.dfm" />
		</edit>
	</target>
	<target name="retirafinal">
		<edit file="${basedir}/geral/defines.inc">
			<search pattern="{$DEFINE FINAL}">
				<delete />
			</search>
			<write file="${basedir}/geral/defines.inc" />
		</edit>
	</target>
